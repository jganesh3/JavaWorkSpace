package LinkedList;

public class SinglyLinkedList<E> {
	
	private Node<E> head;
	
	
	//Constructor
	public SinglyLinkedList(){
		head=null;
	}
	
	
	
	public boolean isEmpty(){
		return head==null;
	}
	
	
	public void addNode(E data){
		
		//check if the head is null;
		if(isEmpty()){
			head=new Node<E>(data);
		}
		
		
	}
	
	
	

}
