package LinkedList;

class Node<E> {
	
	private E data;
	private Node next;
	private Node back;
	

	//default constructor
	public Node(){
		
	}
	// Constructor
	public Node(E x){
		this.data=x;
		this.next=null;
		this.back=null;
		
	}
	
	
	public E getData(){
		return this.data;
		
	}
	
	
	public Node getLeft(){
		return this.next;
	}
	
	public Node getRight(){
		return this.back;
	}
	
	public void setRight(Node x){
		this.back=x;
	}
	
	public void setLeft(Node y){
		this.next=y;
	}
	
	
	
	
	

	

}
